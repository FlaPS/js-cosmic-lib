{
  "source": {
    "include": [
      "src",
      "node_modules/@cosmic-plus/domutils/src/load.js",
      "node_modules/@cosmic-plus/base/src/aliases.js"
    ],
    "exclude": ["src/helpers"]
  },
  "opts": {
    "destination": "./web/doc",
    "encoding": "utf8",
    "readme": "README.md",
    "recurse": true,
    "template": "node_modules/minami",
    "tutorials": "howto"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "default": {
      "includeDate": false
    }
  }
}
