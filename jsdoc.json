{
  "source": {
    "include": [ "src" ]
  },
  "opts": {
    "destination": "./web/doc",
    "encoding": "utf8",
    "readme": "README.md",
    "recurse": true,
    "template": "node_modules/minami",
    "tutorials": "howto"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "default": {
      "includeDate": false
    }
  }
}
